{
  "name": "animatedemo",
  "version": "1.0.0",
  "description": "debug panels for javascript (designed for game development)",
  "main": "index.js",
  "dependencies": {
    "highlight.js": "^9.6.0",
    "penner": "^0.1.3",
    "pixi.js": "^4.0.1",
    "uglify-js": "^2.7.3",
    "yy-angle": "^1.0.0",
    "yy-animate": "^1.0.0",
    "yy-color": "^1.0.6",
    "yy-debug": "^1.0.8",
    "yy-renderer": "^1.0.0",
    "yy-update": "^1.0.0"
  },
  "devDependencies": {
    "uglifyjs": "^2.4.10",
    "browserify": "^13.1.1"
  },
  "scripts": {
    "test": "npm run clear-server && node_modules/.bin/live-server",
    "run": "npm run build && npm run chrome",
    "build": "node_modules/.bin/browserify *.js -u index.compiled.js > index.compiled.js",
    "compress": "node_modules/.bin/browserify *.js -u index.compiled.js && node_modules/.bin/uglifyjs -mc > index.compiled.js",
    "@": "----------------------------------------------------------------------------------",
    "chrome": "open -a 'Google Chrome'",
    "clear-server": "killall node || true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidfig/animate.git"
  },
